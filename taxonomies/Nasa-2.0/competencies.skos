 <skos:Concept rdf:about='urn:comp:27'>
  <skos:prefLabel>Safety Engineering and Assurance</skos:prefLabel>
  <skos:altLabel>SAFENG</skos:altLabel>
  <skos:broader rdf:resource='urn:comp:1381'/>
